{
  "name": "@hi18n/eslint-plugin",
  "version": "0.1.12",
  "description": "Message internationalization meets immutability and type-safety - ESLint plugin",
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "format",
    "formatting",
    "translate",
    "translation",
    "hi18n",
    "eslint",
    "eslintplugin"
  ],
  "homepage": "https://github.com/wantedly/hi18n#readme",
  "bugs": {
    "url": "https://github.com/wantedly/hi18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wantedly/hi18n.git",
    "directory": "packages/eslint-plugin"
  },
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "!dist/*.tsbuildinfo",
    "src/**/*",
    "!src/**/*.test.ts"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "babel": "babel -x .ts src -d dist --source-maps --ignore '**/*.d.ts,**/*.test.ts'",
    "build": "yarn babel && yarn tsc",
    "clean": "rimraf dist",
    "fmt": "prettier . -w",
    "fmt:check": "prettier . -c",
    "lint": "eslint 'src/**/*.ts' .eslintrc.js babel.config.js",
    "prepack": "yarn clean && yarn build",
    "test": "vitest",
    "tsc": "tsc --build"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@typescript-eslint/utils": "^5.22.0",
    "core-js-pure": "^3.23.2",
    "eslint-visitor-keys": "^3.3.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@hi18n/core": "*",
    "@hi18n/react": "*",
    "@hi18n/react-context": "*",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.6",
    "@types/node": "^17.0.32",
    "@typescript-eslint/parser": "^5.39.0",
    "@typescript-eslint/types": "^5.39.0",
    "babel-plugin-polyfill-corejs3": "^0.6.0",
    "eslint": "^8.24.0",
    "eslint-plugin-eslint-plugin": "^5.0.6",
    "eslint-plugin-node": "^11.1.0",
    "espree": "^9.4.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
