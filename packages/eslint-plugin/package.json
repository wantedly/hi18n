{
  "name": "@hi18n/eslint-plugin",
  "version": "0.1.13",
  "description": "Message internationalization meets immutability and type-safety - ESLint plugin",
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "format",
    "formatting",
    "translate",
    "translation",
    "hi18n",
    "eslint",
    "eslintplugin"
  ],
  "homepage": "https://github.com/wantedly/hi18n#readme",
  "bugs": {
    "url": "https://github.com/wantedly/hi18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wantedly/hi18n.git",
    "directory": "packages/eslint-plugin"
  },
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "cjs/**/*",
    "src/**/*",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "main": "./cjs/dist/index.js",
  "exports": {
    "require": "./cjs/dist/index.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "build": "$npm_execpath run build:esm && $npm_execpath run build:cjs",
    "build:cjs": "tsc -p tsconfig.build-cjs.json",
    "build:esm": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist cjs/dist",
    "fmt": "prettier . -w",
    "fmt:check": "prettier . -c",
    "lint": "eslint 'src/**/*.ts' .eslintrc.cjs",
    "prepack": "$npm_execpath run clean && $npm_execpath run build",
    "test": "vitest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.22.0",
    "eslint-visitor-keys": "^3.3.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hi18n/core": "*",
    "@hi18n/react": "*",
    "@hi18n/react-context": "*",
    "@types/eslint": "^8.4.6",
    "@types/node": "^17.0.32",
    "@typescript-eslint/parser": "^5.39.0",
    "@typescript-eslint/types": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-plugin-eslint-plugin": "^5.0.6",
    "eslint-plugin-node": "^11.1.0",
    "espree": "^9.4.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
