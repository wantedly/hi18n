{
  "name": "@hi18n/react",
  "version": "0.1.7",
  "description": "Message internationalization meets immutability and type-safety - runtime for React",
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "format",
    "formatting",
    "translate",
    "translation",
    "hi18n",
    "react",
    "reactjs"
  ],
  "homepage": "https://github.com/wantedly/hi18n#readme",
  "bugs": {
    "url": "https://github.com/wantedly/hi18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wantedly/hi18n.git",
    "directory": "packages/react"
  },
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.8.7",
  "files": [
    "dist/**/*",
    "!dist/*.tsbuildinfo",
    "src/**/*",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "babel:cjs": "babel -x .ts,.tsx src -d dist --source-maps --ignore '**/*.d.ts,**/*.test.ts,**/*.test.tsx'",
    "babel:esm": "babel -x .ts,.tsx src -d dist --source-maps --ignore '**/*.d.ts,**/*.test.ts,**/*.test.tsx' --config-file ./babel-esm.config.js --out-file-extension .mjs",
    "build": "yarn babel:cjs && yarn babel:esm && yarn tsc",
    "clean": "rimraf dist",
    "fmt": "prettier . -w",
    "fmt:check": "prettier . -c",
    "lint": "eslint 'src/**/*.tsx' .eslintrc.js babel.config.js babel-esm.config.js",
    "prepack": "yarn clean && yarn build",
    "test": "vitest",
    "tsc": "tsc --build"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@hi18n/core": "workspace:^0.1.12",
    "@types/react": "^16.8.3 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "core-js-pure": "^3.23.2"
  },
  "peerDependencies": {
    "@hi18n/react-context": "workspace:^0.1.2",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@hi18n/react-context": "workspace:^0.1.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react-dom": "^19.1.9",
    "babel-plugin-polyfill-corejs3": "^0.6.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^26.1.0",
    "prettier": "^2.7.1",
    "pretty-format": "^30.0.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2",
    "vitest": "^3.2.4"
  }
}
