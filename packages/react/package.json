{
  "name": "@hi18n/react",
  "version": "0.1.5",
  "description": "Message internationalization meets immutability and type-safety - runtime for React",
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "format",
    "formatting",
    "translate",
    "translation",
    "hi18n",
    "react",
    "reactjs"
  ],
  "homepage": "https://github.com/wantedly/hi18n#readme",
  "bugs": {
    "url": "https://github.com/wantedly/hi18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wantedly/hi18n.git",
    "directory": "packages/react"
  },
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.2.2",
  "files": [
    "dist/**/*",
    "!dist/*.tsbuildinfo",
    "src/**/*",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "babel:cjs": "babel -x .ts,.tsx src -d dist --source-maps --ignore '**/*.d.ts,**/*.test.ts,**/*.test.tsx'",
    "babel:esm": "babel -x .ts,.tsx src -d dist --source-maps --ignore '**/*.d.ts,**/*.test.ts,**/*.test.tsx' --config-file ./babel-esm.config.js --out-file-extension .mjs",
    "build": "yarn babel:cjs && yarn babel:esm && yarn tsc",
    "clean": "rimraf dist",
    "fmt": "prettier . -w",
    "fmt:check": "prettier . -c",
    "lint": "eslint 'src/**/*.tsx' .eslintrc.js babel.config.js babel-esm.config.js",
    "prepack": "yarn clean && yarn build",
    "test": "jest",
    "tsc": "tsc --build"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@hi18n/core": "workspace:^0.1.9",
    "@types/react": "^16.8.3 || ^17.0.0 || ^18.0.0",
    "core-js-pure": "^3.23.2"
  },
  "peerDependencies": {
    "@hi18n/react-context": "workspace:^0.1.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@hi18n/react-context": "workspace:^0.1.0",
    "@jest/globals": "^28.1.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "babel-jest": "^28.1.3",
    "babel-plugin-polyfill-corejs3": "^0.5.3",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expect": "^28.1.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "pretty-format": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "resolver": "ts-jest-resolver",
    "testEnvironment": "jsdom"
  }
}
